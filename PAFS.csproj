<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net8.0</TargetFramework>
    <PublishSingleFile>true</PublishSingleFile>
    <SelfContained>true</SelfContained>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
	<LangVersion>latest</LangVersion>
	<RollForward>LatestMajor</RollForward>
	<Copyright>2022</Copyright>
	<PackageProjectUrl>https://github.com/ShadowTheHedgehogHacking/PAFS</PackageProjectUrl>
	<RepositoryUrl>https://github.com/ShadowTheHedgehogHacking/PAFS</RepositoryUrl>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="AFSLib" Version="1.1.1" />
  </ItemGroup>

</Project>
